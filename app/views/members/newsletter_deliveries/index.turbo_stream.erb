<%= turbo_stream.append "newsletters" do %>
  <%= render partial: "members/newsletters/newsletter", collection: @deliveries, as: :delivery, locals: { member: current_member } %>
<% end %>

<%= turbo_stream.replace "show-more" do %>
  <% if @next_offset %>
    <%= render "show_more" %>
  <% else %>
    <turbo-frame id="show-more"></turbo-frame>
  <% end %>
<% end %>
