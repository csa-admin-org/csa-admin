@media only screen and (max-width: 768px) {
  table.index_table {
    overflow: hidden;
  }

  table.index_table,
  table.index_table tr,
  table.index_table th,
  table.index_table td,
  table.index_table thead,
  table.index_table tbody {
    display: block;
    text-align: left !important;
    width: 100% !important;
  }

  table.index_table tr {
    padding: 20px;
  }

  table.index_table th,
  table.index_table td {
    background: transparent !important;
    border: 0 !important;
    border-radius: 0 !important;
    padding: 5px 0 !important;
  }

  table.index_table th:empty,
  table.index_table td:empty {
    display: none;
  }

  table.index_table tbody tr {
    border-top: 1px solid #e2e4e8;
  }
}

.attributes_table,
.panel_contents {
  overflow: hidden;
  margin: 0 20px;
}

.attributes_table table,
.panel_contents table {
  width: 100%;
}

.attributes_table th,
.attributes_table td,
.panel_contents th,
.panel_contents td {
  padding: 8px 4px 8px 0;
  text-align: left;
}

.attributes_table th,
.panel_contents th {
  width: 200px;
}

.attributes_table tr + tr th,
.attributes_table tr + tr td,
.panel_contents tr + tr th,
.panel_contents tr + tr td {
  border-top: 1px solid #f1f2f4;
}

.attributes_table .empty,
.panel_contents .empty {
  font-size: 12px;
  opacity: 0.5;
  text-transform: uppercase;
}

@media only screen and (max-width: 480px) {
  .attributes_table th,
  .attributes_table td,
  .panel_contents th,
  .panel_contents td {
    display: block;
    padding: 0 0 5px;
    width: 100%;
  }

  .attributes_table tr + tr th,
  .panel_contents tr + tr th {
    padding: 20px 0 5px;
  }

  .attributes_table tr + tr td,
  .panel_contents tr + tr td {
    border-top: 0;
  }
}

.attributes_table {
  margin: 0;
}

.attributes_table ul {
  margin: 0;
  padding: 0;
  list-style: none;
}

.attributes_table ul li {
  margin-top: 5px;
}

.attributes_table ul li:first-child {
  margin-top: 0;
}

.tabs .nav-tabs {
  border: 1px solid #e2e4e8;
  border-top-left-radius: 6px;
  border-top-right-radius: 6px;
  padding: 6px 15px 0;
}

.tabs .nav-tabs:after {
  clear: both;
  content: "";
  display: table;
}

.tabs .nav-tabs li {
  float: left;
  padding: 9px 16px;
  transition: background-color 250ms;
}

.tabs .nav-tabs li:not(:first-of-type):not(.ui-tabs-active) {
  border-left: 1px solid rgba(226, 228, 232, 0.5);
}

.tabs .nav-tabs li.ui-tabs-active {
  background-color: #f9f9f9;
  border: 1px solid #e2e4e8;
  border-bottom-width: 0;
  border-radius: 6px 6px 0 0;
  font-weight: bold;
  margin-bottom: -1px;
  padding: 9px 15px;
}

.tabs .nav-tabs li a {
  outline: none;
  text-decoration: none;
}

.tabs .ui-tabs-panel > fieldset.inputs {
  border-top: 0;
  border-top-left-radius: 0;
  border-top-right-radius: 0;
}

.table_tools {
  margin-bottom: 20px;
}

.table_tools:after {
  clear: both;
  content: "";
  display: table;
}

.table_tools .dropdown_menu {
  margin-right: 20px;
  float: left;
}

.table_tools .scopes {
  float: left !important;
  width: auto !important;
}

.table_tools .scopes {
  border: 1px solid #e2e4e8;
  border-radius: 6px;
  font-size: 14px;
  list-style: none;
  margin: 0;
  overflow: hidden;
  padding: 0;
  font-weight: 400;
  color: #727272;
}

.table_tools .scopes ul {
  list-style-type: none;
  margin: 0;
  padding: 0;
}

.table_tools .scopes:after {
  clear: both;
  content: "";
  display: table;
}

.table_tools .scope {
  float: left;
  text-align: center;
}

.table_tools .scope.selected a {
  background-color: #ebecef;
  color: #000000;
}

.table_tools .scope.selected a .count {
  opacity: 0.75;
}

.table_tools .scope a {
  background-color: #ffffff;
  display: block;
  padding: 10px 20px;
  text-decoration: none;
}

.table_tools .scope:not(.selected) a:hover {
  background: #ffffff;
}

.table_tools .scope .count {
  font-weight: normal;
  letter-spacing: -1px;
  opacity: 0.5;
}

.table_tools .scope + .scope {
  border-left: 1px solid #e2e4e8;
}

@media only screen and (max-width: 480px) {
  .table_tools {
    margin-bottom: 20px;
    text-align: center;
  }

  .table_tools .batch_actions_selector,
  .table_tools .scopes {
    float: none;
    margin: 0 0 20px;
  }

  .table_tools .scopes,
  .table_tools .scope {
    float: none !important;
    width: 100% !important;
  }

  .table_tools .scope + .scope {
    border-top: 1px solid #e2e4e8;
    border-left: 0;
  }
}

.unsupported_browser {
  padding: 10px 30px;
  background-color: #fff8ef;
  border-bottom: 3px solid #ffb75d;
  margin: 50px 0 -50px;
  text-align: center;
}

.unsupported_browser h1 {
  font-weight: bold;
}

body.logged_out #active_admin_content {
  padding: 0 0 20px;
  overflow: hidden;
}

body.logged_out #content_wrapper {
  margin: 70px auto;
  padding: 0;
  max-width: 500px;
  width: 100%;
}

body.logged_out .flash {
  margin: 0;
}

body.logged_out h2 {
  margin: 0;
  padding: 10px 20px;
}

body.logged_out fieldset {
  border: 0;
  margin: 0;
}

body.logged_out .actions {
  float: right;
  margin-right: 6px;
}

body.logged_out #login a {
  font-size: 80%;
  float: right;
  margin-right: 20px;
}

body.logged_out .flash {
  border-radius: 6px;
  border-width: 1px;
  margin-bottom: 20px;
}

@media only screen and (max-width: 767px) {
  body.logged_out #wrapper {
    padding: 10px 0px;
  }

  body.logged_out #content_wrapper {
    margin: 0 auto;
    max-width: 450px;
  }
}
